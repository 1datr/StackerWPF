<Window x:Class="WpfStackerLibrary.PasswWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Введите пароль" Height="145" Width="272" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Column="0" Grid.Row="0">Логин:</Label>
        <Label Grid.Column="0" Grid.Row="1">Пароль:</Label>
        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5,5,5,5" Text="admin"></TextBlock>
        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5,5,5,5"></TextBlock>
        <Button Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="2" Margin="55,5,55,5">Войти</Button>
    </Grid>
</Window>
