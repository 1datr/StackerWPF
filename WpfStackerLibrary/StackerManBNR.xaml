        <Border HorizontalAlignment="Stretch" Padding="5,5,5,5" VerticalAlignment="Stretch" BorderThickness="1" CornerRadius="5" BorderBrush="#FF0C0C0C">
            <StackPanel Orientation="Vertical">
                <WrapPanel  HorizontalAlignment="Stretch" VerticalAlignment="Top"  Name="dockPanel1" >

                    <Button  HorizontalAlignment="Right" Width="75" Margin="5,5,5,5"  Background="#FF5A260D"  FontWeight="Bold" BorderBrush="{x:Null}" Click="Button_Click_1" Style="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=my:StackerManBNR, AncestorLevel=1}, Path=PowerBtnBrush}">
                        <Button.Resources>
                            <ResourceDictionary x:Name="PowerBtnBrushes">
                                <Style x:Key="true">
                                    <Setter Property="TextBlock.Foreground" Value="AntiqueWhite"></Setter>
                                </Style>
                                <Style x:Key="false">
                                    <Setter Property="TextBlock.Foreground" Value="Gray"></Setter>
                                </Style>
                            </ResourceDictionary>
                        </Button.Resources>
                        <Button.Template>
                            <ControlTemplate TargetType="{x:Type Button}">
                                <TextBlock Name="tbONOFF"  Background="#FF5A260D" FontSize="16" Padding="20,3">OFF</TextBlock>
                                <ControlTemplate.Triggers>
                                    <DataTrigger Binding="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=my:StackerManBNR, AncestorLevel=1}, Path=Power}" Value="True">
                                        <Setter TargetName="tbONOFF" Property="Text" Value="ON" />
                                        <Setter TargetName="tbONOFF" Property="Foreground" Value="Yellow"></Setter>
                                    </DataTrigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>
                        <Button Height="25"  Content="Переподключить" Click="Button_Click"></Button>
                <Button Content="Специальное управление" HorizontalAlignment="Right" Width="150"  Name="btnSpec" Height="25" Margin="5,0,5,5" Click="btnSpec_Click" />
                    <WrapPanel.Resources>
                        <ResourceDictionary x:Name="PowerBtnCaptions" >
                            <base:String x:Key="true">ON</base:String>
                            <base:String x:Key="false">OFF</base:String>
                        </ResourceDictionary>
                    </WrapPanel.Resources>
                </WrapPanel>

                <WrapPanel >
                    <StackPanel Orientation="Horizontal">
                    <Label>X:</Label>
                        <TextBlock Padding="0,5,0,5" Name="tbPosX" Text="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=my:StackerManBNR, AncestorLevel=1}, Path=WorkParams.X}"></TextBlock>
                    </StackPanel>
                    
                    <StackPanel Orientation="Horizontal">
                    <Label>Y:</Label>
                        <TextBlock  Padding="0,5,0,5" Name="tbPosY" Text="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=my:StackerManBNR, AncestorLevel=1}, Path=WorkParams.Y}"></TextBlock>
                    </StackPanel>

                    <StackPanel Orientation="Horizontal">
                    <Label>Z:</Label>
                        <TextBlock Padding="0,5,0,5" Name="tbPosZ" Text="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=my:StackerManBNR, AncestorLevel=1}, Path=WorkParams.Z}"></TextBlock>
                    </StackPanel>
                    </WrapPanel>

                <WrapPanel>
                    <Label>РЕЖИМ:</Label>
                    <Label Content="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=my:StackerManBNR, AncestorLevel=1}, Path=PLCMode}"></Label>
                </WrapPanel>

                <TextBlock TextWrapping="Wrap" Text="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=my:StackerManBNR, AncestorLevel=1}, Path=StackerState}"></TextBlock>
                

            </StackPanel>
            
            
            
